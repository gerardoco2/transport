
<h1>Twilio Confs</h1>

<table>
  <thead>
    <tr>
      <th>Acct</th>
      <th>Auth token</th>
      <th>Phone number</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twilio_confs.each do |twilio_conf| %>
      <tr>
        <td><%= twilio_conf.acct_id %></td>
        <td><%= twilio_conf.auth_token %></td>
        <td><%= twilio_conf.phone_number %></td>
        <td><%= twilio_conf.status %></td>
        <td><%= link_to 'Show', twilio_conf %></td>
        <td><%= link_to 'Edit', edit_twilio_conf_path(twilio_conf) %></td>
        <td><%= link_to 'Destroy', twilio_conf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twilio Conf', new_twilio_conf_path %>
